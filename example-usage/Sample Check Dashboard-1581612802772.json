{
  "__inputs": [
    {
      "name": "DS_INFLUXDB",
      "label": "InfluxDB",
      "description": "",
      "type": "datasource",
      "pluginId": "influxdb",
      "pluginName": "InfluxDB"
    }
  ],
  "__requires": [
    {
      "type": "panel",
      "id": "flant-statusmap-panel",
      "name": "Statusmap",
      "version": "0.2.0"
    },
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "6.4.2"
    },
    {
      "type": "datasource",
      "id": "influxdb",
      "name": "InfluxDB",
      "version": "1.0.0"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "cards": {
        "cardHSpacing": 2,
        "cardMinWidth": 5,
        "cardRound": null,
        "cardVSpacing": 2
      },
      "color": {
        "cardColor": "#b4ff00",
        "colorScale": "sqrt",
        "colorScheme": "interpolateGnYlRd",
        "defaultColor": "#757575",
        "exponent": 0.5,
        "mode": "discrete",
        "thresholds": [
          {
            "color": "#73BF69",
            "tooltip": "OK",
            "value": "0"
          },
          {
            "color": "#F2495C",
            "tooltip": "FAIL",
            "value": "1"
          }
        ]
      },
      "data": {
        "decimals": null,
        "unitFormat": "short"
      },
      "datasource": "${DS_INFLUXDB}",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "highlightCards": true,
      "id": 2,
      "legend": {
        "show": true
      },
      "nullPointMode": "as empty",
      "options": {},
      "targets": [
        {
          "alias": "$tag_check",
          "groupBy": [
            {
              "params": [
                "$__interval"
              ],
              "type": "time"
            },
            {
              "params": [
                "check"
              ],
              "type": "tag"
            },
            {
              "params": [
                "null"
              ],
              "type": "fill"
            }
          ],
          "measurement": "check_status_code",
          "orderByTime": "ASC",
          "policy": "default",
          "refId": "A",
          "resultFormat": "time_series",
          "select": [
            [
              {
                "params": [
                  "upper"
                ],
                "type": "field"
              },
              {
                "params": [],
                "type": "max"
              }
            ]
          ],
          "tags": [
            {
              "key": "app",
              "operator": "=",
              "value": "myapp-chaperone"
            }
          ]
        }
      ],
      "timeFrom": null,
      "timeShift": null,
      "title": "Checks Status",
      "tooltip": {
        "show": true
      },
      "type": "flant-statusmap-panel",
      "useMax": true,
      "xAxis": {
        "labelFormat": "%a %m/%d",
        "minBucketWidthToShowWeekends": 4,
        "show": true,
        "showCrosshair": true,
        "showWeekends": true
      },
      "yAxis": {
        "show": true,
        "showCrosshair": false
      },
      "yAxisSort": "metrics"
    }
  ],
  "schemaVersion": 20,
  "style": "dark",
  "tags": [],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "",
  "title": "Sample Check Dashboard",
  "uid": "QeiYomQWk",
  "version": 3
}